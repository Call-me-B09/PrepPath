{
  "expo": {
    "name": "PrepPath",
    "slug": "preppath",
    "scheme": "preppath",
    "version": "1.0.0",

    "web": {
      "favicon": "./assets/icon.png"
    },

    "experiments": {
      "tsconfigPaths": true
    },

    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",

    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "black"
    },

    "assetBundlePatterns": ["**/*"],

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    }
  }
}
